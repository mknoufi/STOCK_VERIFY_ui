{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": true
  },
  "include": [
    "src/constants/**/*.ts",
    "src/components/AppHeader.tsx",
    "src/components/ProInput.tsx",
    "src/navigation/**/*.ts",
    "src/navigation/**/*.tsx",
    "src/screens/**/*.ts",
    "src/screens/**/*.tsx",
    "app/scaffold.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
